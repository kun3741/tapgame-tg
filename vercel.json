{
  "version": 2,
  "builds": [
      {
          "src": "api/routes/user.js",
          "use": "@vercel/node"
      },
      {
          "src": "api/routes/update.js",
          "use": "@vercel/node"
      },
      {
          "src": "public/**",
          "use": "@vercel/static"
      }
  ],
  "rewrites": [
    { "source": "/(.*)", "destination": "/api" }
  ],
  "installCommand": "npm i cors express mongoose dotenv node-telegram-bot-api path"
}
